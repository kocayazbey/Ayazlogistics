{
  "name": "ayazlogistics",
  "version": "1.0.0",
  "description": "AyazLogistics - Advanced Warehouse and Transportation Management System",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:performance": "artillery run test/performance/load-test.yml",
    "test:coverage": "jest --coverage --coverageReporters=json,html,text,lcov",
    "test:coverage:report": "node test/coverage/generate-coverage-report.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test": "NODE_ENV=test drizzle-kit migrate",
    "db:seed": "ts-node src/database/seed.ts",
    "db:seed:test": "NODE_ENV=test ts-node src/database/seed.ts",
    "db:seed:dev": "NODE_ENV=development ts-node src/database/seed.ts",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all",
    "k8s:apply": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "k8s:logs": "kubectl logs -f deployment/ayazlogistics-backend",
    "k8s:port-forward": "kubectl port-forward service/ayazlogistics-backend 3000:3000",
    "docs:generate": "swagger-jsdoc -d swaggerDef.js src/**/*.ts -o swagger.json",
    "docs:serve": "swagger-ui-serve swagger.json",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "clean": "rimraf dist coverage",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/websockets": "^10.2.8",
    "@sendgrid/mail": "^8.1.6",
    "@socket.io/redis-adapter": "^8.3.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.2",
    "axios-retry": "^4.5.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.4.3",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "drizzle-orm": "^0.29.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "iyzipay": "^2.0.55",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mathjs": "^12.4.3",
    "ml-regression": "^6.3.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.0",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.7.4",
    "stripe": "^14.17.0",
    "uuid": "^9.0.1",
    "webpack": "^5.90.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@playwright/test": "^1.40.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/iyzipay": "^2.0.3",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "artillery": "^2.0.0",
    "codecov": "^3.8.3",
    "cypress": "^13.6.0",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "nyc": "^15.1.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayazlogistics/ayazlogistics.git"
  },
  "keywords": [
    "logistics",
    "warehouse",
    "transportation",
    "management",
    "wms",
    "tms",
    "nestjs",
    "typescript"
  ],
  "author": "AyazLogistics Team",
  "license": "MIT"
}
